<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="cj.product">

    <configlet
        title="Cj.com product."
        action_id="cj.product.cjconfiglet"
        appId="cj.product"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@cjProductControlPanel"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>

